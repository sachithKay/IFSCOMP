<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE filesystem PUBLIC "-//NetBeans//DTD Filesystem 1.2//EN"
                            "http://www.netbeans.org/dtds/filesystem-1_2.dtd">
<filesystem>
    <folder name="Templates">
        <folder name="Other">
            <file name="deployIniTemplate.ini" url="nbresloc:/org/kay/ini/deployIniTemplate.ini">
                <!--org.kay.ini-->
                <attr boolvalue="true" name="template"/>
                <attr
                    bundlevalue="org.kay.ini.Bundle#HTMLtemplate_displayName" name="displayName"/>
                <attr name="iconBase" stringvalue="org/kay/ini/iconbase.png"/>
                <attr name="instantiatingWizardURL" urlvalue="nbresloc:/org/kay/ini/description.html"/>
                <attr name="javax.script.ScriptEngine" stringvalue="freemarker"/>
                <attr boolvalue="true" name="requireProject"/>
            </file>
        </folder>
    </folder>
    <folder name="Actions">
        <folder name="Bugtracking">
            <file name="org-kay-ini-SomeAction.instance">
                <!--org.kay.ini.Launch-->
                <attr name="displayName" stringvalue="View Connections"/>
                <attr methodvalue="org.openide.awt.Actions.context" name="instanceCreate"/>
                <attr name="type" stringvalue="org.openide.loaders.DataObject"/>
                <attr methodvalue="org.openide.awt.Actions.inject" name="delegate"/>
                <attr name="injectable" stringvalue="org.kay.ini.Launch"/>
                <attr name="selectionType" stringvalue="EXACTLY_ONE"/>
                <attr name="iconBase" stringvalue="org/kay/ini/icon.png"/>
                <attr boolvalue="false" name="noIconInMenu"/>
            </file>
        </folder>
    </folder>
    <folder name="Loaders">
        <folder name="folder">
            <folder name="any">
                <folder name="Actions">
                    <file name="org-kay-ini-SomeAction.shadow">
                        <!--org.kay.ini.Launch-->
                        <attr name="originalFile" stringvalue="Actions/Bugtracking/org-kay-ini-SomeAction.instance"/>
                        <attr intvalue="0" name="position"/>
                    </file>
                    <file name="org-kay-ini-SomeAction-separatorAfter.instance">
                        <!--org.kay.ini.Launch-->
                        <attr name="instanceCreate" newvalue="javax.swing.JSeparator"/>
                        <attr intvalue="50" name="position"/>
                    </file>
                </folder>
            </folder>
        </folder>
    </folder>
</filesystem>
